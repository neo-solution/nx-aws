                                "output": "."
                            }
                        ],
                        "srcRootForCompilationRoot": "packages/s3"
                    },
                    "outputs": ["{options.outputPath}"]
                },
                "pack": {
                    "builder": "@nrwl/workspace:run-commands",
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "self"
                        }
                    ],
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npm pack dist/packages/nx-aws-s3",
                                "forwardAllArgs": false
                            }
                        ]
                    }
                },
                "publish": {
                    "builder": "@nrwl/workspace:run-commands",
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "self"
                        }
                    ],
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npm publish dist/packages/nx-aws-s3 --access public --tag $NPM_TAG",
                                "forwardAllArgs": false
                            }
                        ]
                    }
                }
            }
        },
        "s3-e2e": {
            "projectType": "application",
            "root": "e2e/s3-e2e",
            "sourceRoot": "e2e/s3-e2e/src",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/nx-plugin:e2e",
                    "options": {
                        "target": "s3:build",
                        "npmPackageName": "@neo-solution/nx-aws-s3",
                        "pluginOutputPath": "dist/packages/nx-aws-s3",
                        "jestConfig": "e2e/s3-e2e/jest.config.js"
                    }
                }
            }
        },
        "sam": {
