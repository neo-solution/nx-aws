                            "projects": "self"
                        }
                    ],
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npm publish dist/packages/nx-aws-sam --access public --tag $NPM_TAG",
                                "forwardAllArgs": false
                            }
                        ]
                    }
                }
            }
        },
        "sam-e2e": {
            "projectType": "application",
            "root": "e2e/sam-e2e",
            "sourceRoot": "e2e/sam-e2e/src",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/nx-plugin:e2e",
                    "options": {
                        "target": "sam:build",
                        "npmPackageName": "@neo-solution/nx-aws-sam",
                        "pluginOutputPath": "dist/packages/nx-aws-sam",
                        "jestConfig": "e2e/sam-e2e/jest.config.js"
                    }
                }
            }
        },
        "workspace": {
            "architect": {
                "semver": {
                    "builder": "@jscutlery/semver:version",
                    "options": {
                        "syncVersions": true
                    }
                }
            },
            "root": "."
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/workspace"
    }
}
