{
    "version": 1,
    "projects": {
        "core": {
            "root": "packages/core",
            "sourceRoot": "packages/core/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "packages/core/**/*.ts",
                            "packages/core/**/*.spec.ts",
                            "packages/core/**/*.spec.tsx",
                            "packages/core/**/*.spec.js",
                            "packages/core/**/*.spec.jsx",
                            "packages/core/**/*.d.ts"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/core/jest.config.js",
                        "passWithNoTests": true
                    },
                    "outputs": ["coverage/packages/core"]
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
                        "outputPath": "dist/packages/nx-aws-core",
                        "tsConfig": "packages/core/tsconfig.lib.json",
                        "packageJson": "packages/core/package.json",
                        "main": "packages/core/src/index.ts",
                        "assets": [
                            "packages/core/*.md",
                            {
                                "input": "./packages/core/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
                            },
                            {
                                "input": "./packages/core",
                                "glob": "collection.json",
                                "output": "."
                            },
                            {
                                "input": "./packages/core",
                                "glob": "builders.json",
                                "output": "."
                            }
                        ],
                        "srcRootForCompilationRoot": "packages/core"
                    },
                    "outputs": ["{options.outputPath}"]
                },
                "pack": {
                    "builder": "@nrwl/workspace:run-commands",
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "self"
                        }
                    ],
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npm pack dist/packages/nx-aws-core",
                                "forwardAllArgs": false
                            }
                        ]
                    }
                },
                "publish": {
                    "builder": "@nrwl/workspace:run-commands",
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "self"
                        }
                    ],
                    "options": {
                        "parallel": false,
                        "commands": [
                            {
                                "command": "npm publish dist/packages/nx-aws-core --access public --tag $NPM_TAG",
                                "forwardAllArgs": false
                            }
                        ]
                    }
                }
            }
        },
        "s3": {
            "root": "packages/s3",
            "sourceRoot": "packages/s3/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "packages/s3/**/*.ts",
                            "packages/s3/**/*.spec.ts",
                            "packages/s3/**/*.spec.tsx",
                            "packages/s3/**/*.spec.js",
                            "packages/s3/**/*.spec.jsx",
                            "packages/s3/**/*.d.ts"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/s3/jest.config.js",
                        "passWithNoTests": true
                    },
                    "outputs": ["coverage/packages/s3"]
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
                        "outputPath": "dist/packages/nx-aws-s3",
                        "tsConfig": "packages/s3/tsconfig.lib.json",
                        "packageJson": "packages/s3/package.json",
                        "main": "packages/s3/src/index.ts",
                        "assets": [
                            "packages/s3/*.md",
                            {
                                "input": "./packages/s3/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
                            },
                            {
                                "input": "./packages/s3",
                                "glob": "collection.json",
                                "output": "."
                            },
                            {
